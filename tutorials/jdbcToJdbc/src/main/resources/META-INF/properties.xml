  <!-- # rows that the operator can retrieve in a window -->
  <property>
    <name>dt.operator.JdbcOutput.prop.batchSize</name>
    <value>500</value>
  </property>

  <property>
    <name>dt.operator.JdbcOutput.prop.maxLengthOfRollingFile</name>
    <value>101200</value>
  </property>
  <!-- Output Table name -->
  <property>
    <name>dt.operator.JdbcOutput.prop.tableName
    </name>
    <value>events</value>
  </property>
  
  <!-- POJO class -->
  <property>
    <name>dt.operator.JdbcOutput.port.input.attr.TUPLE_CLASS
    </name>
    <value>com.example.mydtapp.PojoEvent</value>
  </property>

  <property>
    <name>dt.operator.JdbcOutput.attr.PARTITIONER</name>
    <value>com.datatorrent.common.partitioner.StatelessPartitioner:1</value>
  </property>
</configuration>

